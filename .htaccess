php_flag allow_url_include on
php_flag output_buffering on

RewriteEngine on

ReWriteRule ^Home$ index.php [NC]
ReWriteRule ^Register$ index.php?sPage=Register [NC]
ReWriteRule ^Login$ index.php?sPage=Login [NC]
ReWriteRule ^PerfilTrabalhador/(.*)$ index.php?sPage=PerfilTrabalhador&iUsuarioId=$1
ReWriteRule ^PerfilUsuario index.php?sPage=PerfilUsuario [NC]
ReWriteRule ^Administrador index.php?sPage=Administrador [NC]